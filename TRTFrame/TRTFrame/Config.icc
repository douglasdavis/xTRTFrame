// Inline implementation of TRTF::Config files.

inline bool TRTF::Config::useGRL() const { return m_useGRL; }
inline bool TRTF::Config::usePRW() const { return m_usePRW; }

inline const std::vector<std::string>& TRTF::Config::GRLFiles()     const { return m_GRLFiles;     }
inline const std::vector<std::string>& TRTF::Config::PRWConfFiles() const { return m_PRWConfFiles; }
inline const std::vector<std::string>& TRTF::Config::PRWLumiFiles() const { return m_PRWLumiFiles; }
